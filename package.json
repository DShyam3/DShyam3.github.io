{
  "name": "terminal-portfolio",
  "homepage": "https://DShyam3.github.io/DShyam3.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd Terminal_Portfolio && vite",
    "build": "cd Terminal_Portfolio && tsc && vite build",
    "lint": "cd Terminal_Portfolio && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "cd Terminal_Portfolio && vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d Terminal_Portfolio/dist",
    "update-mediaboard": "node scripts/update-mediaboard.js",
    "update-mediaboard:force": "node scripts/update-mediaboard.js --force",
    "update-mediaboard:daily": "node scripts/update-mediaboard.js --daily --platforms",
    "update-mediaboard:test": "node scripts/update-mediaboard.js --force --webhook"
  },
  "dependencies": {
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.74.3",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gh-pages": "^6.3.0",
    "lucide-react": "^0.330.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^6.2.6"
  }
}
